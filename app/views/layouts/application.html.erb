<!DOCTYPE html>
<html>
<head>
  <title>Forest</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
	<header>
				<h1 class="pull-left"><%= link_to "Wild Road Calculus", root_path  %></h1>
			<% if current_user %>
				<p class="pull-right"><%= link_to  "Levels", pages_levels_path %> |
				<%= link_to "Profile", user_path(current_user)  %> |
				<%= link_to "Logout", sessions_delete_path  %></p>
			<% else %>
			
			<p><%= link_to "Login", new_session_path  %> | <%= link_to "Create User", new_user_path  %></p>
			<% end %>	
	</header>

	
	<%= flash[:notice]  %>
<%= yield %>
 <footer>
 </footer>
</body>
</html>
